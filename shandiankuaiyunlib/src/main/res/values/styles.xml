<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--roundedimageview-->
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>
    <!--roundedimageview-->



    <!--Main-->
    <!--首页底部栏-->
    <style name="common_bottom">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@drawable/main_bottom_word_selector</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:background">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">0.5</item>
    </style>
    <!--首页底部栏-->
    <!--Main-->
    <!--Main index renrenkuaidi-->
    <style name="renrenkuaidi_bottom">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@drawable/main_index_renrenwuliu_word_selector</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:background">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">0.5</item>
    </style>
    <!--Main index renrenkuaidi-->


</resources>